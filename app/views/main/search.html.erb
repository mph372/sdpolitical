<div class="row">
  <div class="col">
    <h1>People</h1>
    <% @people.each do |person| %>
      <p><%= link_to "#{person.first_name} #{person.last_name}", person %></p>
    <% end %>
  </div>

    <div class="col">
    <h1>Jurisdictions</h1>
    <% @jurisdictions.each do |jurisdiction| %>
      <p><%= link_to "#{jurisdiction.name}", jurisdiction %></p>
    <% end %>
  </div>
</div>