<p id="notice"><%= notice %></p>

<h1>Registration Snapshots</h1>

<table>
  <thead>
    <tr>
      <th>Snapshot date</th>
      <th>Total registered</th>
      <th>Registered dem</th>
      <th>Registered rep</th>
      <th>Registered other</th>
      <th>Statisticaldata</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_snapshots.each do |registration_snapshot| %>
      <tr>
        <td><%= registration_snapshot.snapshot_date %></td>
        <td><%= registration_snapshot.total_registered %></td>
        <td><%= registration_snapshot.registered_dem %></td>
        <td><%= registration_snapshot.registered_rep %></td>
        <td><%= registration_snapshot.registered_other %></td>
        <td><%= registration_snapshot.statistical_datum %></td>
        <td><%= link_to 'Show', registration_snapshot %></td>
        <td><%= link_to 'Edit', edit_registration_snapshot_path(registration_snapshot) %></td>
        <td><%= link_to 'Destroy', registration_snapshot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Snapshot', new_registration_snapshot_path %>
