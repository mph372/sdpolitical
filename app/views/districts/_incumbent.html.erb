<%# Shared Incumbent Partial %>

  <% if @district.person.present? %>
    <%= render 'shared/incumbent', person: @district.person, page_source: 'district', show_header: true %>
  <% else %>
    <%= render partial: 'shared/vacant', locals: { person: @district.person, page_source: "district", show_header: true } %>
  <% end %>

