    <%= form.text_field :other_percent %>
  </div>

  <div class="field">
    <%= form.label :newsom_percent %>
    <%= form.text_field :newsom_percent %>
  </div>

  <div class="field">
    <%= form.label :cox_percent %>
    <%= form.text_field :cox_percent %>
  </div>

  <div class="field">
    <%= form.label :clinton_percent %>
    <%= form.text_field :clinton_percent %>
  </div>

  <div class="field">
    <%= form.label :trump_percent %>
    <%= form.text_field :trump_percent %>
