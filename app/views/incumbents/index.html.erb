<p id="notice"><%= notice %></p>

<h1>Incumbents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incumbents.each do |incumbent| %>
      <tr>
        <td><%= link_to 'Show', incumbent %></td>
        <td><%= link_to 'Edit', edit_incumbent_path(incumbent) %></td>
        <td><%= link_to 'Destroy', incumbent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incumbent', new_incumbent_path %>
